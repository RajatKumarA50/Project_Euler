public class Q21 {

	public static void main(String[] args) 
	{
		System.out.println(new Q21().run());
	}
	public String run() {
		int sum = 0;
		for (int i = 1; i < 10000; i++) {
			if (isAmicable(i))
				sum += i;
		}
		return Integer.toString(sum);
	}
	
	
	private static boolean isAmicable(int n) {
		int m = divisorSum(n);
		return m != n && divisorSum(m) == n;
	}
